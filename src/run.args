--dataset_name rte

--pretrained_model_name bert-base-cased

--output_dir /tmp/rte/
--do_train
--do_eval
--evaluation_strategy epoch
--per_device_train_batch_size 32
--per_device_eval_batch_size 32
--learning_rate 2e-5
--weight_decay 0.01
--fp16
--load_best_model_at_end
--metric_for_best_model accuracy
--n_trials 10
--save_total_limit 1
